{
    "env": {
        "browser": true,
        "commonjs":true,
        "es6": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "settings": {
        "react": {
            "version": "^17.0.2"
        }
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:jsx-a11y/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react", 
        "jsx-a11y"
    ]
}
